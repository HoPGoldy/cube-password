{
  "name": "node-fullstack-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:HoPGoldy/node-fullstack-template.git",
  "author": "HoPGoldy <hpgless@outlook.com>",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run lint && npm run server:build && npm run client:build",
    "start": "npx cross-env PORT=3700 node dist/index.js",
    "lint": "eslint src --ext .ts",
    "server:dev": "ts-node-dev -r tsconfig-paths/register --project tsconfig.server.json src",
    "server:build": "tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "client:dev": "vite",
    "client:build": "tsc && vite build"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "fs-extra": "^10.1.0",
    "history": "^5.3.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-jwt": "^4.0.3",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "lokijs": "^1.5.12",
    "nanoid": "3.3.4",
    "qs": "^6.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-vant": "^2.0.0-alpha.46"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/lodash": "^4.14.182",
    "@types/lokijs": "^1.5.7",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^17.0.17",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.1",
    "less": "^4.1.3",
    "nodemon": "^2.0.18",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.5.2",
    "vite": "^2.8.0"
  },
  "license": "MIT"
}
